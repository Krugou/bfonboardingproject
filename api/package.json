{
  "name": "apibfinno",
  "version": "1.0.0",
  "description": "api for bfinno",
  "main": "bfinno/main.js",
  "scripts": {
    "start": "node bfinno/main.js",
    "build": "npm run tsc",
    "node": "node bfinno/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "apidev": "npm i && concurrently \"tsc --watch\" \"nodemon --delay 1s bfinno/main.js\"",
    "dev": "npm i && concurrently \"tsc --watch\" \"nodemon --delay 1s bfinno/main.js\" \"cd .. && npm run start\"",
    "tsc": "tsc",
    "tscW": "tsc --watch",
    "copyApi": "python copy_api.py"
  },
  "author": "bfinno",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "nodemon": "^3.1.7",
    "openai": "^4.68.4",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  },
  "engines": {
    "node": "22.x"
  }
}
