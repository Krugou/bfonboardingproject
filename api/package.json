  "dependencies": {
    "@types/cors": "^2.8.17",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "nodemon": "^3.1.4",
    "openai": "^4.67.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21"
  }
}
