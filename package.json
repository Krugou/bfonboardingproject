{
  "name": "bfonboarding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm i && npm run dev",
    "herokuStart": "cd api && npm i && npm run start",
    "dev": "next dev --turbo --port 3008",
    "easyDev": "concurrently \"npm run tsc:watch\" \"npm run dev\" \"npm run autoOpen\" ",
    "autoOpen": "wait-on http://localhost:3008 && start http://localhost:3008",
    "build": "next build",
    "startB": "next start",
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "copyApi": "cd api && npm run copyApi"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.1.8",
    "@mui/material": "^6.1.8",
    "@xyflow/react": "^12.3.5",
    "firebase": "^11.0.2",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flag-kit": "^1.1.1",
    "react-hook-form": "^7.53.2",
    "react-select": "^5.8.3",
    "react-toastify": "^10.0.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.3",
    "@types/firebase": "^3.2.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.1"
  }
}
