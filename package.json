{
  "name": "bfonboarding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm i && npm run easyDev",
    "dev": "next dev --turbo --port 3008",
    "easyDev": "concurrently \"npm run tsc:watch\" \"npm run dev\" \"npm run autoOpen\" ",
    "autoOpen": "wait-on http://localhost:3008 && start http://localhost:3008",
    "build": "next build",
    "startB": "next start",
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "firebase": "^11.0.1",
    "next": "^15.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flag-kit": "^1.1.1",
    "react-select": "^5.8.2",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.1",
    "@types/firebase": "^3.2.3",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-config-next": "^15.0.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "wait-on": "^8.0.1"
  }
}
